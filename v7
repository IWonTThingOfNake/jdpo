local esp_ch = Section_C_E:CreateToggle({
   Name = "Survivor ESP",
   CurrentValue = false,
   Flag = "monkeyblackness", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
      flags.plrESP_C = Value

      if Value == true then
            while flags.plrESP_C == true do
               for i,v in pairs(workspace.Characters:GetChildren()) do
                  if not v:FindFirstChild("_KillerModel") then
                     esp(v,{Color = ChasingESPSurv.Color}, true)
                  end
               end
               task.wait(1)
            end
      end
   end,
})

local GenESP_btn = v1:CreateToggle({
   Name = "Generator ESP",
   CurrentValue = false,
   Flag = "genes", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
      flags.genESP = Value

      if Value == true then
         while flags.genESP == true do
            for i,v in pairs(workspace.Map.Ingame:FindFirstChildOfClass("Model"):GetChildren()) do
               if v.Name == "Generator" then
                  esp(v,{Color = GenESP.Color, AddGui = " | "..tostring(v.Progress.Value).."%"}, true)
                elseif v.Name == "FakeGenerator" then
                  esp(v,{Color = FakeGenESP.Color, AddGui = " | "..tostring(v.Progress.Value).."%"}, true)
              end
            end
            task.wait(1)
         end
      end
   end,
})

local Leaderboard = v0:CreateToggle({
   Name = "Leaderboard (When match starts)",
   CurrentValue = false,
   Flag = "sixSeven", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
      flags.lb = Value

      if Value == true then
         while flags.lb == true do
            if game.Players.LocalPlayer.Character.Parent.Name ~= "Spectating" then
               game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList,true)
            end
            task.wait(1)
         end
      else
         game.StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList,false)
      end
   end,
})


local KillerESP_Button = v1:CreateToggle({
   Name = "Killer ESP",
   CurrentValue = false,
   Flag = "Toggle312391231", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
      flags.KillerESP_Forsaken = Value

      if Value == true then
         while flags.KillerESP_Forsaken == true do
            for i,v in pairs(workspace.Players.Killers:GetChildren()) do
               esp(v,{Color = KillerESP_Color_Forsaken.Color, AddGui = " | "..game.Players:GetPlayerFromCharacter(v).Name}, true)
            end
            task.wait(1)
         end
      end
   end,
})


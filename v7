local DOD_ABILITIES = {
    "Adrenaline",
    "Caretaker",
    "Cloak",
    "Block",
    "Dash",
    "BonusPad",
    "Hotdog",
    "Revolver",
    "Taunt",
    "Banana",
      "Punch",
      "Carepad"
}

local UseAbility_DOD = game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("RemoteFunctions"):WaitForChild("UseAbility")

for i, v in ipairs(DOD_ABILITIES) do
    a0:CreateButton({
        Name = "Use Ability: " .. v,
        Callback = function()
            UseAbility_DOD:InvokeServer(v)
        end
    })
end


local InfStam_DOD = m0:CreateToggle({
   Name = "Infinite Stamina",
   CurrentValue = false,
   Flag = "SevenSix", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
      flags.infStam_dod = Value

      if Value == true then
         while flags.infStam_dod == true do
            if game.Players.LocalPlayer.Character:GetAttribute("MaxStamina") then
               game.Players.LocalPlayer.Character:SetAttribute("MaxStamina", 99999)
            end
            task.wait(1)
         end
      end
   end,
})

local IgnoreSlowness = m0:CreateToggle({
   Name = "Ignore Slowness Debuffs",
   CurrentValue = false,
   Flag = "I:67", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
      flags.ignSlow = Value

      if Value == true then
         while flags.ignSlow == true do
            if game.Players.LocalPlayer.Character:GetAttribute("WalkSpeedModifier") then
               game.Players.LocalPlayer.Character:SetAttribute("WalkSpeedModifier", 0)
            end
            task.wait(0.1)
         end
      end
   end,
})

